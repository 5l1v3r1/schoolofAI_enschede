   "source": [
    "import tensorflow as tf\n",
    "\n",
    "x = tf.constant([[1.,2.]])\n",
    "negMatrix = tf.negative(x)\n",
    "\n",
    "num1 = tf.constant(5)\n",
    "num2 = tf.constant(10)\n",
    "num3 = tf.multiply(num1,num2)\n",
    "\n",
    "\n",
    "with tf.Session() as sess:\n",
    "    result = sess.run([negMatrix, num3])\n",
    "    \n",
    "print(result)\n",
    "\n",
    "sess.close()\n"
   ]
