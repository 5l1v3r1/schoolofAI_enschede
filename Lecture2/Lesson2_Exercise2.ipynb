    "import tensorflow as tf\n",
    "\n",
    "\n",
    "a = tf.add(1,2)\n",
    "\n",
    "b = tf.add(3,4)\n",
    "\n",
    "c = tf.multiply (a,b)\n",
    "\n",
    "with tf.Session() as sess:\n",
    "    writer = tf.summary.FileWriter(\"./output2\", sess.graph)\n",
    "    print(sess.run(c))\n",
    "    writer.close()\n",
    "\n"
